{
  "lock": false,
  "tasks": {
    "dev": "deno run -A npm:vite dev",
    "build": "deno run -A npm:vite build --base=./"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/preact",
    "lib": [
      "deno.window",
      "dom",
      "ESNext"
    ]
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-inferrable-types"
      ]
    }
  },
  "exclude": [
    "dist/"
  ]
}